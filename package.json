    },
    "devDependencies": {
        "@types/node": "^24.2.0",
        "@types/punycode": "^2.1.4",
        "rimraf": "^6.0.1",
        "tsx": "^4.20.3",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.0"
    }
}
